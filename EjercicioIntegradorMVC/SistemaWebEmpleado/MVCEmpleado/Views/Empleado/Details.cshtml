@model MVCEmpleado.Models.Empleado
@{
    ViewData["Title"] = "Details";
}

<section>
    <h2>Detalles de: @Model.Nombre @Model.Apellido</h2>
    <article>
        <p>
            @Html.DisplayNameFor(model => model.DNI): 
            @Html.DisplayFor(model => model.DNI)
        </p>
        <p>
            @Html.DisplayNameFor(model => model.FechaNacimiento): 
            @Html.DisplayFor(model => model.FechaNacimiento)
        </p>
        <p>
            @Html.DisplayNameFor(model => model.Legajo): 
            @Html.DisplayFor(model => model.Legajo)
        </p>
        <p>
            @Html.DisplayNameFor(model => model.Titulo): 
            @Html.DisplayFor(model => model.Titulo)
        </p>
    </article>
    @Html.ActionLink("Volver", "Index", "Empleado")
</section>

@section SideBar{
    <aside class="aside">
        <h3>Administración de empleados</h3>
    </aside>
}


